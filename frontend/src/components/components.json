[
  {
    "name": "FlexGauge",
    "description": "This classic component is often used by managers to evaluate weird key performance indicators. For some obscure reason, managers like to have this component added to their dahsboards. Not all chart libraries provide a nice Speedometer component, so I decided to make one in Vue.",
    "props": [
      {
        "range": [10, 10, 10, 10, 10, 10, 10, 10, 10, 10],
        "animationSpeed": "2"
      }
    ],
    "controls": [
      {
        "name": "v-slider",
        "props": {
          "label": "min",
          "min": -100,
          "max": 1
        },
        "model": "min",
        "default": -100
      },
      {
        "name": "v-slider",
        "props": {
          "label": "max",
          "min": 5,
          "max": 100
        },
        "model": "max",
        "default": 100
      },
      {
        "name": "v-slider",
        "props": {
          "label": "score",
          "min": -100,
          "max": 100
        },
        "model": "score",
        "default": 0
      },
      {
        "name": "v-slider",
        "props": {
          "label": "speed",
          "min": 0.0001,
          "max": 0.5,
          "step": "0.0001"
        },
        "model": "acceleration",
        "default": 0.1
      },
      {
        "name": "v-switch",
        "props": {
          "label": "refresh button"
        },
        "model": "showRefreshButton",
        "default": true
      },
      {
        "name": "v-switch",
        "props": {
          "label": "hide measures"
        },
        "model": "hideMeasures",
        "default": false
      },
      {
        "name": "v-switch",
        "props": {
          "label": "gradient"
        },
        "model": "gradient",
        "default": false
      }
    ]
  },
  {
    "name": "ContributionGrid",
    "description": "Remember Github's contribution chart ?",
    "props": [
      {
        "dataset": [
          {
            "logDate": "Thu Jun 09 2022 07:50:35 GMT+0200"
          },
          {
            "logDate": "Thu Jun 09 2022 07:50:35 GMT+0200"
          },
          {
            "logDate": "Tue Jun 07 2022 07:50:35 GMT+0200"
          },
          {
            "logDate": "Wed Jun 08 2022 07:50:35 GMT+0200"
          }
        ]
      }
    ],
    "controls": [
      {
        "name": "v-switch",
        "props": {
          "label": "color legend"
        },
        "model": "hideLegend",
        "default": false
      },
      {
        "name": "v-switch",
        "props": {
          "label": "tooltip"
        },
        "model": "hideTooltip",
        "default": false
      },
      {
        "name": "v-switch",
        "props": {
          "label": "outline"
        },
        "model": "outlined",
        "default": true
      },
      {
        "name": "v-switch",
        "props": {
          "label": "x legend"
        },
        "model": "hideXLegend",
        "default": false
      },
      {
        "name": "v-switch",
        "props": {
          "label": "y legend"
        },
        "model": "hideYLegend",
        "default": false
      },
      {
        "name": "v-switch",
        "props": {
          "label": "rounded"
        },
        "model": "rounded",
        "default": false
      },
      {
        "name": "v-switch",
        "props": {
          "label": "show today"
        },
        "model": "showToday",
        "default": true
      }
    ]
  }
]
