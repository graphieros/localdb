          "label": "score",
          "min": -100,
          "max": 100
        },
        "model": "score",
        "default": 0
      },
      {
        "name": "v-slider",
        "props": {
          "label": "speed",
          "min": 0.0001,
          "max": 0.5,
          "step": "0.0001"
        },
        "model": "acceleration",
        "default": 0.1
      },
      {
        "name": "v-switch",
        "props": {
          "label": "refresh button"
        },
        "model": "showRefreshButton",
        "default": true
      },
      {
        "name": "v-switch",
        "props": {
          "label": "hide measures"
        },
