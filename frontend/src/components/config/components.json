      {
        "range": [10, 10, 10, 10, 10, 10, 10, 10, 10, 10],
        "animationSpeed": "2"
      }
    ],
    "controls": [
      {
        "name": "v-slider",
        "props": {
          "label": "min",
          "min": -100,
          "max": 1
        },
        "model": "min",
        "default": -100
